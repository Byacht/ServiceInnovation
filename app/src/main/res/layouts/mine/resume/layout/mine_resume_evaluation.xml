<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout
        android:id="@+id/smalltitle3"
        android:layout_width="match_parent"
        android:layout_height="48dp">
        <View
            android:id="@+id/mine_resume_spot3"
            android:layout_width="8dp"
            android:layout_height="8dp"
            android:background="#FF6C72"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="30dp"
            android:layout_centerVertical="true"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="自我评价"
            android:fontFamily="@font/noto_sans"
            android:textColor="#121212"
            android:textSize="16sp"
            android:layout_toRightOf="@id/mine_resume_spot3"
            android:layout_marginLeft="15dp"
            android:layout_centerVertical="true"/>
    </RelativeLayout>
    <EditText
        android:id="@+id/mine_resume_evaluation_et"
        android:layout_width="345dp"
        android:layout_height="143dp"
        android:background="#FFFFFF"
        android:layout_below="@id/smalltitle3"
        android:layout_centerHorizontal="true"/>
    <TextView
        android:id="@+id/mine_resume_evaluation_tv"
        android:layout_width="345dp"
        android:layout_height="wrap_content"
        android:background="#FFFFFF"
        android:textColor="#9E9E9E"
        android:textSize="12dp"
        android:fontFamily="@font/noto_sans"
        android:layout_below="@id/mine_resume_evaluation_et"
        android:layout_centerHorizontal="true"/>
</RelativeLayout>